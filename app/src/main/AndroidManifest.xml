<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naild2d.android" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".PackageListActivity"
            android:label="@string/title_activity_package_list" />
        <activity
            android:name=".PackageDetailActivity"
            android:label="@string/title_activity_package_detail" >
        </activity>
        <activity
            android:name=".PendingOrderListActivity"
            android:label="@string/title_activity_pending_order_list" >
        </activity>
        <activity
            android:name=".OrderDetailsActivity"
            android:label="@string/title_activity_order_details" >
        </activity>
        <activity
            android:name=".OrderAddressActivity"
            android:label="@string/title_activity_order_address" >
        </activity>
        <activity
            android:name=".OrderCommentActivity"
            android:label="@string/title_activity_order_comment" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".UserProfileActivity"
            android:label="@string/title_activity_user_profile" />
        <activity
            android:name=".ForgetPasswordActivity"
            android:label="@string/title_activity_forget_password" />
        <activity
            android:name=".RegisterUserActivity"
            android:label="@string/title_activity_register_user" />
        <activity
            android:name=".LocationActivity"
            android:label="@string/title_activity_location_activty">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OrderCouponActivity"
            android:label="@string/title_activity_order_coupon">
        </activity>
    </application>

</manifest>
